{
  "expo": {
    "name": "MadiAssist",
    "slug": "MadiAssist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/assets/MediAssistLogo.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/assets/MediAssistTittleLogo.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.noorfatima.finalMadiAssist"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.noorfatima.finalMadiAssist"
    },
    "web": {
      "favicon": "./assets/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/assets/MediAssistTittleLogo.jpeg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}